<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane maxHeight="600.0" maxWidth="900.0" minHeight="600.0" minWidth="900.0" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.MenuController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="900.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/bk.jpg" />
         </image>
      </ImageView>
      <Button layoutX="384.0" layoutY="300.0" mnemonicParsing="false" onAction="#btStartPressed" prefHeight="50.0" prefWidth="132.0" style="-fx-background-color: #FF84C1; -fx-background-radius: 30;" text="開始遊戲" textFill="#ffd8f8">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Button>
      <Button layoutX="384.0" layoutY="411.0" mnemonicParsing="false" onAction="#btEndPressed" prefHeight="50.0" prefWidth="132.0" style="-fx-background-color: #FF84C1; -fx-background-radius: 30;" text="結束遊戲" textFill="#ffd8f8">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Button>
      <Label layoutX="258.0" layoutY="132.0" text="史萊姆進化論" textFill="#ffd8f8">
         <font>
            <Font name="System Bold" size="64.0" />
